{
  "config": {
    "repo_address": "https://repo3.eclipsecontent.org",
    "repo_insecure": false,
    "secretsmanager_address": "https://secretsmanager-dev.eclipse.org",
    "secretsmanager_path": "repo3.eclipse.org",
    "defaults": {}
  },
  "projects": [
    {
      "id": "automotive.tractusx",
      "template": "custom",
      "config": {
        "project_id": "automotive.tractusx",
        "repositories": [
          {
            "type": "helm"
          }
        ]
      }
    },
    {
      "id": "technology.cbi",
      "template": "custom",
      "config": {
        "project_id": "technology.cbi",
        "repositories": [
          {
            "type": "docker"
          }
        ],
        "proxies": [
          {
            "type": "docker"
          },
          {
            "type": "npm"
          },
          {
            "type": "pypi"
          },
          {
            "type": "helm"
          },
          {
            "type": "maven2",
            "proxy": {
              "remote_url": "http://localhost:8081"
            }
          }
        ]
      }
    },
    {
      "id": "technology.csi",
      "template": "custom",
      "config": {
        "project_id": "technology.csi",
        "proxies": [
          {
            "name": "sonatype_iq",
            "type": "docker"
          },
          {
            "name": "sonatype_iq",
            "type": "maven2"
          }
        ],
        "repositories": [
          {
            "name": "sonatype_iq",
            "type": "docker",
            "env": [
              "production",
              "staging"
            ]
          },
          {
            "name": "sonatype_iq",
            "type": "maven2",
            "env": [
              "release",
              "snapshot"
            ]
          }
        ]
      }
    }
  ]
}
