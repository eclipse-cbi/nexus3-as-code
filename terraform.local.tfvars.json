{
  "repo_address": "http://localhost:8081",
  "repo_insecure": false,
  "secretsmanager_address": "https://secretsmanager-staging.eclipse.org",
  "defaults": {
    "repositories": {
      "docker": {
        "storage": {
          "blob_store_name": "docker"
        }
      }
    },
    "proxies": {
      "docker": {
        "storage": {
          "blob_store_name": "docker"
        }
      }
    },
    "groups": {
      "docker": {
        "storage": {
          "blob_store_name": "docker"
        }
      }
    }
  },
  "projects": [
    {
      "project_id": "automotive.tractusx",
      "repositories": [
        {
          "type": "helm"
        }
      ],
      "proxies": [
        {
          "type": "helm"
        }
      ]
    },
    {
      "project_id": "technology.cbi",
      "repositories": [
        {
          "type": "docker"
        }
      ],
      "proxies": [
        {
          "type": "docker"
        },
        {
          "type": "npm"
        },
        {
          "type": "pypi"
        },
        {
          "type": "helm"
        },
        {
          "type": "maven2",
          "proxy": {
            "remote_url": "http://localhost:8081"
          }
        }
      ]
    },
    {
      "project_id": "technology.csi",
      "groups": [
        {
          "type": "docker",
          "name": "sonatype_iq",
          "docker": {
            "force_basic_auth": false,
            "http_port": 8080,
            "https_port": 8433,
            "v1_enabled": true
          },
          "storage": {
            "blob_store_name": "default",
            "strict_content_type_validation": true
          }
        }
      ],
      "proxies": [
        {
          "name": "sonatype_iq",
          "type": "docker"
        },
        {
          "name": "sonatype_iq",
          "type": "maven2"
        }
      ],
      "repositories": [
        {
          "name": "sonatype_iq",
          "type": "docker",
          "env": [
            "production",
            "staging"
          ]
        },
        {
          "name": "sonatype_iq",
          "type": "maven2",
          "env": [
            "release",
            "snapshot"
          ]
        }
      ]
    }
  ],
  "secretsmanager_path": "repo3.eclipse.org"
}
