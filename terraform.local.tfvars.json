{
   "defaults": {
      "groups": {
         "docker": {
            "storage": {
               "blob_store_name": "docker"
            }
         }
      },
      "proxies": {
         "docker": {
            "storage": {
               "blob_store_name": "docker"
            }
         }
      },
      "repositories": {
         "docker": {
            "storage": {
               "blob_store_name": "docker"
            }
         }
      }
   },
   "projects": [
      {
         "project_id": "automotive.tractusx",
         "proxies": [
            {
               "type": "helm"
            }
         ],
         "repositories": [
            {
               "type": "helm"
            }
         ]
      },
      {
         "project_id": "technology.cbi",
         "proxies": [
            {
               "type": "docker"
            },
            {
               "type": "npm"
            },
            {
               "type": "pypi"
            },
            {
               "type": "helm"
            },
            {
               "proxy": {
                  "remote_url": "http://localhost:8081"
               },
               "type": "maven2"
            }
         ],
         "repositories": [
            {
               "type": "docker"
            }
         ]
      },
      {
         "groups": [
            {
               "docker": {
                  "force_basic_auth": false,
                  "http_port": 8080,
                  "https_port": 8433,
                  "v1_enabled": true
               },
               "name": "sonatype_iq",
               "storage": {
                  "blob_store_name": "default",
                  "strict_content_type_validation": true
               },
               "type": "docker"
            }
         ],
         "project_id": "technology.csi",
         "proxies": [
            {
               "name": "sonatype_iq",
               "type": "docker"
            },
            {
               "name": "sonatype_iq",
               "type": "maven2"
            }
         ],
         "repositories": [
            {
               "env": [
                  "production",
                  "staging"
               ],
               "name": "sonatype_iq",
               "type": "docker"
            },
            {
               "env": [
                  "release",
                  "snapshot"
               ],
               "name": "sonatype_iq",
               "type": "maven2"
            }
         ]
      }
   ],
   "repo_address": "http://localhost:8081",
   "repo_insecure": false,
   "secretsmanager_address": "https://secretsmanager-staging.eclipse.org",
   "secretsmanager_path": "repo3.eclipse.org"
}
